<config>
    <global>
        <cache>
            <request_processors>
                <ee>
                    <sortOrder>50</sortOrder>
                    <class>Enterprise_PageCache_Model_Processor</class>
                </ee>
            </request_processors>
        </cache>
        <cache_advanced>
            <full_page>
                <backend_options>
                    <cache_dir>full_page_cache</cache_dir>
                </backend_options>
            </full_page>
        </cache_advanced>
        <skip_process_modules_updates>0</skip_process_modules_updates>
        <di>
            <preferences>
                <Enterprise_PageCache_Model_Processor_RestrictionInterface>Enterprise_PageCache_Model_Processor_Restriction</Enterprise_PageCache_Model_Processor_RestrictionInterface>
            </preferences>
            <Enterprise_PageCache_Model_Processor>
                <parameters>
                    <storeManager>Mage_Core_Model_StoreManager_Proxy</storeManager>
                    <metadata>Enterprise_PageCache_Model_Metadata_Proxy</metadata>
                    <requestIdentifier>Enterprise_PageCache_Model_Request_Identifier_Proxy</requestIdentifier>
                </parameters>
            </Enterprise_PageCache_Model_Processor>
            <Enterprise_PageCache_Model_DesignPackage_Rules>
                <parameters>
                    <designChange>Mage_Core_Model_Design_Proxy</designChange>
                    <design>Mage_Core_Model_View_Design_Proxy</design>
                </parameters>
            </Enterprise_PageCache_Model_DesignPackage_Rules>
            <Magento_Http_Handler_Composite>
                <parameters>
                    <handlers>
                        <fpc>
                            <sortOrder>25</sortOrder>
                            <class>Enterprise_PageCache_Model_Http_Handler</class>
                        </fpc>
                    </handlers>
                </parameters>
            </Magento_Http_Handler_Composite>
            <Enterprise_Queue_Helper_Gearman>
                <parameters>
                    <config>Mage_Core_Model_Config</config>
                </parameters>
            </Enterprise_Queue_Helper_Gearman>
        </di>
        <configurators>
            <pageCache>Enterprise_PageCache_Model_ObjectManager_Configurator</pageCache>
            <enterprise_search>Enterprise_Search_Model_ObjectManager_Configurator</enterprise_search>
            <queue>Enterprise_Queue_Model_ParamMapper</queue>
        </configurators>
    </global>
</config>
