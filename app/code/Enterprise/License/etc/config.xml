<config>
    <adminhtml>
        <events>
            <admin_user_authenticate_after>
                <observers>
                    <enterprise_license>
                        <class>Enterprise_License_Model_Observer</class>
                        <method>adminUserAuthenticateAfter</method>
                    </enterprise_license>
                </observers>
            </admin_user_authenticate_after>
            <controller_action_predispatch>
                <observers>
                    <enterprise_license>
                        <class>Enterprise_License_Model_Observer</class>
                        <method>preDispatch</method>
                    </enterprise_license>
                </observers>
            </controller_action_predispatch>
        </events>
    </adminhtml>
</config>
