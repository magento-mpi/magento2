<config>
    <modules>
        <Mage_LoadTest>
            <version>0.1.0</version>
            <depends>
                <Mage_Catalog />
                <Mage_Customer />
                <Mage_Sales />
                <Mage_Tag />
                <Mage_Review />
            </depends>
        </Mage_LoadTest>
    </modules>

    <global>
        <models>
            <loadtest>
                <class>Mage_LoadTest_Model</class>
            </loadtest>
        </models>
        <helpers>
            <loadtest>
                <class>Mage_LoadTest_Helper</class>
            </loadtest>
        </helpers>
    </global>

    <frontend>
        <models>
            <loadtest>
                <class>Mage_LoadTest_Model_LoadTest</class>
            </loadtest>
        </models>
        <routers>
            <loadtest>
                <use>standard</use>
                <args>
                    <module>Mage_LoadTest</module>
                    <frontName>loadtest</frontName>
                </args>
            </loadtest>
        </routers>

        <translate>
            <modules>
                <Mage_LoadTest>
                    <files>
                        <default>Mage_LoadTest.csv</default>
                    </files>
                </Mage_LoadTest>
            </modules>
        </translate>
    </frontend>
</config>