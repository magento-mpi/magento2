<config>
    <acl>
        <resources>
            <resource id="Magento_Adminhtml::admin">
                <resource id="Mage_Catalog::catalog" module="Mage_Catalog" title="Products" sortOrder="30">
                    <resource id="Mage_Catalog::catalog_inventory" module="Mage_Catalog" title="Inventory" sortOrder="10">
                        <resource id="Mage_Catalog::products" title="Catalog" module="Mage_Catalog" sortOrder="10" />
                        <resource id="Mage_Catalog::categories" title="Categories" module="Mage_Catalog" sortOrder="20" />
                    </resource>
                </resource>
                <resource id="Magento_Adminhtml::marketing">
                    <resource id="Magento_Adminhtml::marketing_seo">
                        <resource id="Mage_Catalog::urlrewrite" title="URL Redirects" module="Mage_Catalog" sortOrder="20" />
                    </resource>
                </resource>
                <resource id="Magento_Adminhtml::stores">
                    <resource id="Magento_Adminhtml::stores_settings">
                        <resource id="Magento_Adminhtml::config">
                            <resource id="Mage_Catalog::config_catalog" module="Mage_Catalog" title="Catalog Section" />
                        </resource>
                    </resource>
                    <resource id="Magento_Adminhtml::stores_attributes">
                        <resource id="Mage_Catalog::attributes_attributes" title="Product" module="Mage_Catalog" sortOrder="30" />
                        <resource id="Mage_Catalog::update_attributes" title="Update Attributes" sortOrder="35" />
                        <resource id="Mage_Catalog::sets" title="Product Template" module="Mage_Catalog" sortOrder="40"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
