<config>
    <modules>
        <Mage_Dataflow>
            <version>1.6.0.0</version>
        </Mage_Dataflow>
    </modules>
    <global>
        <models>
            <dataflow>
                <class>Mage_Dataflow_Model</class>
                <resourceModel>dataflow_resource</resourceModel>
            </dataflow>
            <dataflow_resource>
                <class>Mage_Dataflow_Model_Resource</class>
            </dataflow_resource>
        </models>
        <resources>
            <dataflow_setup>
                <setup>
                    <module>Mage_Dataflow</module>
                </setup>
            </dataflow_setup>
        </resources>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <dataflow>
                    <file>dataflow.xml</file>
                </dataflow>
            </updates>
        </layout>
    </adminhtml>
</config>
