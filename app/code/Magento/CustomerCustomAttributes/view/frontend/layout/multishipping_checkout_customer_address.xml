<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <referenceBlock name="customer_address_edit">
        <action method="setTemplate">
            <argument name="template" xsi:type="string">Magento_CustomerCustomAttributes::customer/address/edit.phtml</argument>
        </action>
        <block class="Magento\CustomerCustomAttributes\Block\Form" name="customer_form_user_attributes" template="form/userattributes.phtml">
            <action method="setFormCode">
                <argument name="code" xsi:type="string">customer_address_edit</argument>
            </action>
        </block>
    </referenceBlock>
    <update handle="customer_form_template_handle"/>
</layout>
