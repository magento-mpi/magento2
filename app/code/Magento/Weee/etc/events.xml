<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Event/etc/events.xsd">
    <event name="catalog_entity_attribute_save_before">
        <observer name="weee" instance="Magento\Weee\Model\Observer" method="assignBackendModelToAttribute" shared="false" />
    </event>
    <event name="catalogrule_after_apply">
        <observer name="weee" instance="Magento\Weee\Model\Observer" method="updateDiscountPercents" />
    </event>
    <event name="catalog_product_view_config">
        <observer name="weee" instance="Magento\Weee\Model\Observer" method="updateProductOptions" />
    </event>
    <event name="bundle_product_view_config">
        <observer name="weee" instance="Magento\Weee\Model\Observer" method="updateBundleProductOptions" />
    </event>
</config>
