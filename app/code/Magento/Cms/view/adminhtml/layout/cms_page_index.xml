<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Core/etc/page.xsd">
    <referenceBlock name="head">
        <block class="Magento\Theme\Block\Html\Head\Css" name="autocomplete-css-styles-css">
            <arguments>
                <argument name="file" xsi:type="string">jquery/autocomplete/autocomplete.css</argument>
            </arguments>
        </block>
    </referenceBlock>
    <referenceContainer name="content">
         <block class="Magento\Ui\Listing\View" name="cms_page" template="Magento_Ui::listing/grid/horizontal.phtml">
             <arguments>
                 <argument name="id" xsi:type="string">notificationGrid</argument>
                 <argument name="dataSource" xsi:type="object">Magento\Cms\Model\Resource\Page\Collection</argument>
                 <argument name="default_dir" xsi:type="string">DESC</argument>
                 <argument name="default_sort" xsi:type="string">date_added</argument>
                 <argument name="save_parameters_in_session" xsi:type="string">1</argument>
             </arguments>
             <container name="cms_page_listing_before" label="" htmlTag="div">
                 <block class="Magento\Ui\Listing\Component\Paging\View" template="Magento_Ui::listing/component/paging/default.phtml" />
             </container>
             <container name="cms_page_listing_after" label="" htmlTag="div">
                 <block class="Magento\Ui\Listing\Component\Paging\View" template="Magento_Ui::listing/component/paging/default.phtml" />
             </container>
         </block>
    </referenceContainer>
</page>
