<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" label="RMA Return Create" design_abstraction="custom">
    <referenceContainer name="content">
        <block class="Magento\View\Element\Template" name="magento_rma_item_form_template">
            <block class="Magento\CustomAttributeManagement\Block\Form\Renderer\Text" name="magento_rma_item_form_template_text" as="text" template="Magento_Rma::form/renderer/text.phtml"/>
            <block class="Magento\CustomAttributeManagement\Block\Form\Renderer\Textarea" name="magento_rma_item_form_template_textarea" as="textarea" template="Magento_Rma::form/renderer/textarea.phtml"/>
            <block class="Magento\Rma\Block\Form\Renderer\Select" name="magento_rma_item_form_template_select" as="select" template="form/renderer/select.phtml" cacheable="false"/>
            <block class="Magento\Rma\Block\Form\Renderer\Image" name="magento_rma_item_form_template_image" as="image" template="form/renderer/image.phtml"/>
        </block>
    </referenceContainer>
</layout>
