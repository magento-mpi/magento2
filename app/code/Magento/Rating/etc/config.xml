<config>
    <global>
        <resources>
            <rating_setup>
                <setup>
                    <module>Magento_Rating</module>
                </setup>
            </rating_setup>
        </resources>
    </global>
    <frontend>
        <translate>
            <modules>
                <Magento_Rating>
                    <files>
                        <default>Magento_Rating.csv</default>
                    </files>
                </Magento_Rating>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <translate>
            <modules>
                <Magento_Rating>
                    <files>
                        <default>Magento_Rating.csv</default>
                    </files>
                </Magento_Rating>
            </modules>
        </translate>
        <events>
            <catalog_product_delete_after_done>
                <observers>
                    <rating>
                        <class>Magento_Rating_Model_Observer</class>
                        <method>processProductAfterDeleteEvent</method>
                    </rating>
                </observers>
            </catalog_product_delete_after_done>
        </events>
    </adminhtml>
</config>
