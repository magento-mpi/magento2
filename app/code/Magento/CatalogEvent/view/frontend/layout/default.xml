<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <reference name="head">
        <block class="Magento\Page\Block\Html\Head\Script" name="magento-catalogevent-js-ticker-js" ifconfig="catalog/magento_catalogevent/enabled">
            <arguments>
                <argument name="file" xsi:type="string">Magento_CatalogEvent::js/ticker.js</argument>
            </arguments>
        </block>
        <block class="Magento\Page\Block\Html\Head\Css" name="magento-catalogevent-widgets-css" ifconfig="catalog/magento_catalogevent/enabled">
            <arguments>
                <argument name="file" xsi:type="string">Magento_CatalogEvent::widgets.css</argument>
            </arguments>
        </block>
        <block class="Magento\Page\Block\Html\Head\Script" name="magento-catalogevent-js-carousel-js" ifconfig="catalog/magento_catalogevent/enabled">
            <arguments>
                <argument name="file" xsi:type="string">Magento_CatalogEvent::js/carousel.js</argument>
            </arguments>
        </block>
    </reference>
</layout>
