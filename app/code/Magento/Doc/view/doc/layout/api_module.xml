<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../Core/etc/layout_single.xsd">
    <referenceContainer name="column.main">
        <block class="Magento\Doc\Ui\Widget\Document" name="document_content" template="Magento_Doc::html/widget/document.phtml">
            <arguments>
                <argument name="scheme" xsi:type="string">api/module</argument>
            </arguments>
        </block>
    </referenceContainer>
    <referenceContainer name="column.toolbar">
        <block class="Magento\Doc\Ui\Widget\Toolbar" name="document_toolbar" template="Magento_Doc::html/widget/toolbar.phtml"/>
    </referenceContainer>
    <referenceContainer name="column.sidebar">
        <block class="Magento\Doc\Ui\Widget\Navigation\ModuleMenu" name="modules_menu" template="Magento_Doc::html/widget/navigation/modules.phtml"/>
    </referenceContainer>
</layout>
