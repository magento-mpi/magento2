<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="column.main">
            <block class="Magento\Doc\Ui\Widget\Document\Content" name="document_content" template="Magento_Doc::html/widget/document/content.phtml"/>
        </referenceContainer>
        <referenceContainer name="column.toolbar">
            <block class="Magento\Doc\Ui\Widget\Document\Toolbar" name="document_toolbar" template="Magento_Doc::html/widget/document/toolbar.phtml"/>
        </referenceContainer>
        <referenceContainer name="column.sidebar">
            <block class="Magento\Doc\Ui\Widget\Document\Outline" name="document_outline" template="Magento_Doc::html/widget/document/outline.phtml"/>
        </referenceContainer>
    </body>
</page>
