<config>
    <preference for="Magento\Core\Model\Resource\Setup\Migration" type="Magento\Enterprise\Model\Resource\Setup\Migration" />
    <type name="Magento\Core\Model\View\Design">
        <param name="themes">
            <value>
                <frontend>magento_fixed_width</frontend>
                <adminhtml>magento_backend</adminhtml>
                <install>magento_enterprise</install>
            </value>
        </param>
    </type>
    <type name="Magento\Install\Block\Begin">
        <param name="eulaFile">
            <value>LICENSE_EE.html</value>
        </param>
    </type>
    <type name="Magento\Core\Model\Db\Updater">
        <param name="resourceList">
            <value>
                <enterprise_setup>Magento\Enterprise\Model\Resource\Setup</enterprise_setup>
            </value>
        </param>
    </type>
</config>
