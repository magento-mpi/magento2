<config>
    <virtualType name="Magento\Pbridge\Model\Session\Storage" type="Magento\Session\Storage">
        <param name="namespace">
            <value>magento_pbridge</value>
        </param>
    </virtualType>
    <type name="Magento\Pbridge\Model\Session">
        <param name="storage">
            <instance type="Magento\Pbridge\Model\Session\Storage" />
        </param>
        <param name="sessionName">
            <value>frontend</value>
        </param>
    </type>
    <type name="Magento\Core\Model\Url\SecurityInfo">
        <param name="secureUrlList">
            <array>
                <item key="magento_pbridge_pbridge"><value>/magento_pbridge/pbridge/</value></item>
                <item key="magento_pbridge_payment_profile"><value>/magento_pbridge/payment_profile/</value></item>
            </array>
        </param>
    </type>

    <type name="Magento\Pbridge\Model\Payment\Method\Authorizenet">
        <param name="formBlock">
            <value>Magento\Pbridge\Block\Checkout\Payment\Authorizenet</value>
        </param>
    </type>

    <type name="Magento\Pbridge\Model\Payment\Method\Dibs">
