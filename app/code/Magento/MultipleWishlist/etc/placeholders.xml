<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../FullPageCache/etc/placeholders.xsd">
    <placeholder name="wishlist" code="WISHLIST" cacheLifeTime="86400">
        <container instance="Magento\FullPageCache\Model\Container\Wishlist" />
        <block instance="Magento\MultipleWishlist\Block\Customer\Sidebar"/>
    </placeholder>
    <placeholder name="wishlist_links" code="WISHLISTLINKS" cacheLifeTime="86400">
        <container instance="Magento\FullPageCache\Model\Container\Wishlistlinks" />
        <block instance="Magento\MultipleWishlist\Block\Link" />
    </placeholder>
    <placeholder name="wishlist_list" code="WISHLISTS" cacheLifeTime="86400">
        <container instance="Magento\FullPageCache\Model\Container\Wishlists" />
        <block instance="Magento\MultipleWishlist\Block\Behaviour" />
    </placeholder>
</config>
