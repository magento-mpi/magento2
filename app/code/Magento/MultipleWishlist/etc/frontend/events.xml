<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../lib/Magento/Event/etc/events.xsd">
    <event name="wishlist_delete_commit_after">
        <observer name="magento_fullpagecache" instance="Magento\FullPageCache\Model\Observer" method="registerWishlistChange" />
    </event>
    <event name="wishlist_save_commit_after">
        <observer name="magento_fullpagecache" instance="Magento\FullPageCache\Model\Observer" method="registerWishlistListChange" />
    </event>
</config>
