<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route url="/V1/products/links/types" method="GET">
        <service class="Magento\Catalog\Service\V1\CatalogProductLinkServiceInterface" method="getProductLinkTypes"/>
        <resources>
            <resource ref="Magento_Catalog::catalog"/>
        </resources>
    </route>
    <route url="/V1/products/:productId/links/:type" method="GET">
        <service class="Magento\Catalog\Service\V1\CatalogProductLinkServiceInterface" method="getLinkedProducts"/>
        <resources>
            <resource ref="Magento_Catalog::catalog"/>
        </resources>
    </route>
</routes>
