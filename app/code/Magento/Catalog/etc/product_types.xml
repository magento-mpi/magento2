<config>
    <type name="simple" label="Simple Product" modelInstance="Magento\Catalog\Model\Product\Type\Simple" indexPriority="10">
        <customAttributes>
            <attribute name="refundable" value="true"/>
        </customAttributes>
    </type>
    <type name="virtual" label="Virtual Product" modelInstance="Magento\Catalog\Model\Product\Type\Virtual" indexPriority="20">
        <customAttributes>
            <attribute name="is_real_product" value="false"/>
        </customAttributes>
    </type>

    <composableTypes>
        <type name="simple" />
        <type name="virtual" />
    </composableTypes>
</config>
