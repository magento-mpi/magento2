<validation>
    <entity name="eav_entity">
        <rules>
            <rule name="eav_attribute_data">
                <entity_constraints>
                    <constraint alias="eav_data_validator" class="Magento\Eav\Model\Validator\Attribute\Data" />
                </entity_constraints>
            </rule>
        </rules>
        <groups>
            <group name="form">
                <uses>
                    <use rule="eav_attribute_data"/>
                </uses>
            </group>
        </groups>
    </entity>
</validation>
