<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" label="Customer Checkout by SKU" type="page" parent="checkout_cart_index">
    <referenceBlock name="page.main.title">
        <action method="setPageTitle">
            <argument translate="true" name="title" xsi:type="string">Order by SKU</argument>
        </action>
    </referenceBlock>
    <update handle="customer_account"/>
    <referenceBlock name="my.account.wrapper">
        <block class="Magento\AdvancedCheckout\Block\Customer\Sku" name="checkout.customer.sku" template="widget/sku.phtml">
            <action method="setIsTitleHidden">
                <argument name="value" xsi:type="string">true</argument>
            </action>
            <block class="Magento\Core\Block\Template" template="Magento_AdvancedCheckout::customer/file.phtml"/>
        </block>
    </referenceBlock>
</layout>
