<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../../code/Magento/Core/etc/layout_single.xsd">
    <update handle="customer_account"/>
    <referenceContainer name="my.account.wrapper">
        <block class="Magento\Sales\Block\Order\Info" as="info" name="sales.order.info">
            <block class="Magento\Sales\Block\Order\Info\Buttons" as="buttons" name="sales.order.info.buttons" cacheable="false"/>
        </block>
    </referenceContainer>
    <update handle="sales_order_info_links"/>
    <container name="additional.product.info" label="Additional Product Info"/>
</layout>
