<!-- These are the settings for legacy testing within the company's Magento 2 repository
     (in addition to Magento 2 Core and Magento 2 EE) -->
<config>
    <words>
        <word>Magento_Autoload::</word>
        <word>Magento\Core\Utility\Theme</word>
        <word>@group module:</word>
        <word>Magento\Test\Di\InstanceManager</word>
        <word>\Magento\ObjectManager_Zend</word>
        <word>base_public_url</word>
        <word>secure_public_url}}js/</word>
        <word>\Magento\Core\Model\App_Options</word>
        <word>Magento\Adminhtml\Utility\Controller</word>
        <word>twig</word>
    </words>
    <whitelist>
        <item>
            <path>dev/tests/functional-old/fixture/default</path>
        </item>
        <item>
            <path>dev/tests/functional-old/fixture/enterprise</path>
        </item>
        <item>
            <path>dev/tests/functional-old/testsuite/Enterprise</path>
        </item>
        <item>
            <path>lib/internal/Symfony</path>
            <word>twig</word>
        </item>
    </whitelist>
</config>
