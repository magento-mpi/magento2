<page mca="cms/block" module="Magento_Cms">
    <blocks>
        <messagesBlock>
            <class>Magento\Core\Test\Block\Messages</class>
            <locator>#messages</locator>
            <strategy>css selector</strategy>
        </messagesBlock>
        <gridPageActions>
            <class>Magento\Backend\Test\Block\GridPageActions</class>
            <locator>.page-main-actions</locator>
            <strategy>css selector</strategy>
        </gridPageActions>
        <cmsBlockGrid>
            <class>Magento\Cms\Test\Block\Adminhtml\Block\CmsGrid</class>
            <locator>.grid</locator>
            <strategy>css selector</strategy>
        </cmsBlockGrid>
    </blocks>
</page>
