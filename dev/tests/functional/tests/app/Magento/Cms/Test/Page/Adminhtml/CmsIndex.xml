<page mca="admin/cms_page/index" module="Magento_Cms">
    <blocks>
        <pageActionsBlock>
            <class>Magento\Backend\Test\Block\GridPageActions</class>
            <locator>.page-main-actions</locator>
            <strategy>css selector</strategy>
        </pageActionsBlock>
        <cmsPageGridBlock>
            <class>Magento\Cms\Test\Block\Adminhtml\Page\Grid</class>
            <locator>.grid</locator>
            <strategy>css selector</strategy>
        </cmsPageGridBlock>
        <messagesBlock>
