<page mca="catalog/product/edit" >
    <block>
        <name>productForm</name>
        <class>Magento\Catalog\Test\Block\Adminhtml\Product\ProductForm</class>
        <locator>[id="page:main-container"]</locator>
        <strategy>css selector</strategy>
    </block>
    <block>
        <name>productPageAction</name>
        <class>Magento\Catalog\Test\Block\Adminhtml\Product\ProductPageAction</class>
        <locator>.page-main-actions</locator>
        <strategy>css selector</strategy>
    </block>
    <block>
        <name>message</name>
        <class>Magento\Core\Test\Block\Messages</class>
        <locator>#messages .messages</locator>
        <strategy>css selector</strategy>
    </block>
    <block>
        <name>productAttributeForm</name>
        <class>Magento\Catalog\Test\Block\Adminhtml\Product\Attribute\AttributeForm</class>
        <locator>#create_new_attribute_container</locator>
        <strategy>css selector</strategy>
    </block>
</page>
