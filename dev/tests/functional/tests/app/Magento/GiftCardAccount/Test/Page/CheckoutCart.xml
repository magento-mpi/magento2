<page mca="checkout/cart/index">
    <blocks>
        <giftCardAccountBlock>
            <class>Magento\GiftCardAccount\Test\Block\Checkout\Cart\Giftcardaccount</class>
            <locator>.giftcard</locator>
            <strategy>css selector</strategy>
        </giftCardAccountBlock>
        <checkBlock>
            <class>Magento\GiftCardAccount\Test\Block\Check</class>
            <locator>.giftcard.info</locator>
            <strategy>css selector</strategy>
