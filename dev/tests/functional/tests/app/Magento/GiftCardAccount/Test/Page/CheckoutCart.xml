        <giftCardDiscountBlock>
            <class>Magento\GiftCardAccount\Test\Block\Checkout\Cart\Total</class>
            <locator>.giftcard.totals</locator>
            <strategy>css selector</strategy>
        </giftCardDiscountBlock>
    </blocks>
