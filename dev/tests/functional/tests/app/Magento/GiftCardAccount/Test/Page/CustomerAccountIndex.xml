<page mca="customer/account/index">
    <blocks>
        <accountMenuBlock>
            <class>Magento\Customer\Test\Block\Account\Links</class>
            <locator>.nav.items</locator>
            <strategy>css selector</strategy>
        </accountMenuBlock>
        <redeemBlock>
            <class>Magento\GiftCardAccount\Test\Block\Account\Redeem</class>
            <locator>#giftcard-form</locator>
            <strategy>css selector</strategy>
        </redeemBlock>
        <storeCreditBlock>
            <class>Magento\CustomerBalance\Test\Block\Account\History</class>
            <locator>#maincontent</locator>
            <strategy>css selector</strategy>
        </storeCreditBlock>
        <checkBlock>
            <class>Magento\GiftCardAccount\Test\Block\Check</class>
            <locator>.giftcard.info</locator>
            <strategy>css selector</strategy>
        </checkBlock>
    </blocks>
</page>
