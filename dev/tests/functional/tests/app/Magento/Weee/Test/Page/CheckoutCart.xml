<page mca="checkout/cart/index">
    <blocks>
        <weeeCartBlock>
            <class>Magento\Weee\Test\Block\Cart</class>
            <locator>//div[contains(@class, "column main")]</locator>
            <strategy>xpath</strategy>
        </weeeCartBlock>
        <weeeTotalsBlock>
            <class>Magento\Weee\Test\Block\Cart\Totals</class>
            <locator>#shopping-cart-totals-table</locator>
            <strategy>css selector</strategy>
        </weeeTotalsBlock>
    </blocks>
</page>
