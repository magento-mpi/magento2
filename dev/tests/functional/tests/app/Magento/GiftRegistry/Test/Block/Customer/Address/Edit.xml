<mapping strict="0">
    <wrapper>address</wrapper>
    <fields>
        <firstname/>
        <lastname/>
        <company/>
        <street>
            <selector>[id="address:street1"]</selector>
        </street>
        <city/>
        <region_id>
            <input>select</input>
        </region_id>
        <postcode/>
        <country_id>
            <input>select</input>
        </country_id>
        <telephone/>
        <fax/>
    </fields>
</mapping>
