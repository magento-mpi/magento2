        <strategy>css selector</strategy>
    </block>
    <block>
        <name>payflowLinkCcBlock</name>
        <class>Magento\Payment\Test\Block\Form\PayflowAdvanced\CcLink</class>
        <locator>body</locator>
        <strategy>css selector</strategy>
    </block>
