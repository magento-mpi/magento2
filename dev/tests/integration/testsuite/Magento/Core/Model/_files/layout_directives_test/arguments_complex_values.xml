<layout>
    <block type="Magento_Core_Block_Text" name="block_with_args_complex_values">
        <arguments>
            <one>
                <parameters>
                    <first>1</first>
                    <second>2</second>
                </parameters>
            </one>
            <two>
                <parameter_one>
                    <value>Paramter One</value>
                </parameter_one>
                <parameter_two>2</parameter_two>
            </two>
            <three>
                <extra>
