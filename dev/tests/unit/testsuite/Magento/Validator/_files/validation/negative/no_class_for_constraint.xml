<validation>
    <entity name="test_entity">
        <rules>
            <rule name="test_rule_constraint">
                <entity_constraints>
                    <!-- attribute class is required for tag element -->
                    <constraint alias="eav_validator"/>
                </entity_constraints>
            </rule>
        </rules>
        <groups>
            <group name="test_group">
                <uses>
                    <use rule="test_rule_constraint"/>
                </uses>
            </group>
        </groups>
    </entity>
</validation>
