<validation>
    <entity name="test_entity">
        <rules>
            <rule name="test_rule_entity">
                <property_constraints>
                    <property name="sku">
                        <constraint class="Magento\Validator\Test\NotEmpty"/>
                    </property>
                </property_constraints>
            </rule>
        </rules>
        <groups>
            <!-- attribute name is required for group element -->
            <group>
                <uses>
                    <use rule="test_rule_entity"/>
                </uses>
            </group>
        </groups>
    </entity>
</validation>
