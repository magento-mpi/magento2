<payment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="../../../../../../../../../app/code/Magento/Payment/etc/payment.xsd">
    <credit_cards>
        <type id="SM" order="90">
            <label>Switch/Maestro</label>
        </type>
        <type id="SO" order="61">
            <label>Solo</label>
        </type>
    </credit_cards>
    <groups>
        <group id="paypal">
            <label>PayPal</label>
        </group>
    </groups>
    <methods>
        <method name="checkmo">
            <allow_multiple_address>1</allow_multiple_address>
            <allow_multiple_with_3dsecure>1</allow_multiple_with_3dsecure>
        </method>
    </methods>
</payment>
