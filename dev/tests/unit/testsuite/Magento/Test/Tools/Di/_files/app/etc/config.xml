<config>
    <global>
        <di>
            <Node1 type="Magento\App\Cache">
                <parameters>
                    <storeManager>
                        <instance>customStoreManagerProxy</instance>
                    </storeManager>
                </parameters>
            </Node1>
            <Node2 type ="Magento\App\Action\Context">
                <plugins>
                    <first><instance>Magento\Core\Model\Action\Plugin</instance></first>
                </plugins>
                <parameters>
                    <layoutFactory>
                        <instance>customLayoutFactory</instance>
                    </layoutFactory>
                </parameters>
            </Node2>
            <customStoreManagerProxy>
                <type>Magento\Core\Model\StoreManager\Proxy</type>
            </customStoreManagerProxy>
            <customLayoutFactory>
                <type>Magento\Core\Model\Layout\Factory</type>
            </customLayoutFactory>
        </di>
    </global>
</config>
