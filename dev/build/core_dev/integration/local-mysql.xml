<config>
    <install>
        <date><![CDATA[d-d-d-d-d]]></date>
    </install>
    <crypt>
        <key><![CDATA[k-k-k-k-k]]></key>
    </crypt>
    <db>
        <table_prefix><![CDATA[{{db_table_prefix}}]]></table_prefix>
    </db>
    <connection name="default">
        <host><![CDATA[{{db_host}}]]></host>
        <username><![CDATA[{{db_user}}]]></username>
        <password><![CDATA[{{db_password}}]]></password>
        <dbName><![CDATA[{{db_name}}]]></dbName>
        <initStatements><![CDATA[SET NAMES utf8]]></initStatements>
        <pdoType><![CDATA[]]></pdoType>
        <adapter><![CDATA[Magento_TestFramework_Db_ConnectionAdapter]]></adapter>
        <active>1</active>
    </connection>
    <resource name="default_setup" connection="default"/>
    <session_save><![CDATA[files]]></session_save>
</config>
